#include <stdio.h>

/* Calculates the factorial */
int calcFact(int input){
    
    if (input == 0) 
        return 1;
    else
        return calcFact(input - 1) * input;
}

int main() {

    int inp;
    printf("Enter Input\n");
    scanf("%d", &inp); 
    printf("Factorial of Input = %d", calcFact(inp));
    return 0;
}
