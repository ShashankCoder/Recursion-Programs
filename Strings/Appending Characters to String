#include <stdio.h>
#include <string.h>

int main() {
    
    int length;
    char inp_str[30];
    char element = 'w';
    
    printf("Enter the string\n");
    fgets(inp_str,30,stdin);
    puts(inp_str);
    
    length = strlen(inp_str);
    inp_str[length] = element;
    inp_str[length - 1] = 'o';
    
    puts(inp_str);
    return 0;
}
